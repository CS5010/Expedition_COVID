{
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/__pycache__/**": true,
	},
	"files.watcherExclude": {
		"**/.git/**": true,
		"**/__pycache__/**": true,
	},
	"python.formatting.provider": "autopep8",
	"editor.formatOnSave": false
}